{
  "name": "@progress/telerik-angular-report-viewer",
  "description": "Progress® Telerik® Report Viewer for Angular",
  "version": "25.25.313",
  "author": "Progress",
  "license": "See LICENSE.md",
  "homepage": "https://www.telerik.com/reporting",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/reporting.git",
    "directory": "Source/Code/Telerik.ReportViewer.Web/angular-viewer"
  },
  "bugs": {
    "url": "https://github.com/telerik/reporting/issues/new?labels=bug,FW:angular&template=bug_report.md"
  },
  "keywords": [
    "Progress",
    "Telerik",
    "report",
    "reports",
    "reporting",
    "ReportViewer",
    "Report Viewer",
    "Angular"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/main.js",
  "module": "dist/es/main.js",
  "types": "dist/es/main.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "service": "dotnet run --project ../rest-service/RestService.csproj --launch-profile Development",
    "service:ci": "dotnet run --project ../rest-service/RestService.csproj --launch-profile CI",
    "prebuild": "rimraf dist && node ./scripts/prebuild.js",
    "build:csj": "npx ngc --p tsconfig.cjs.json",
    "build:esm": "npx ngc --p tsconfig.esm.json",
    "build": "npm run build:csj && npm run build:esm",
    "start": "npx ng serve"
  },
  "peerDependencies": {
    "@angular/common": "16 - 19",
    "@angular/core": "16 - 19",
    "jquery": "^1.10.0 || ^2.2.0 || ^3.7.0"
  },
  "dependencies": {
    "@progress/telerik-jquery-report-viewer": "25.25.313"
  }
}
